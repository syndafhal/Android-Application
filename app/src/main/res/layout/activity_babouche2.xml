<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/zaoa"
    android:layout_height="match_parent"
    tools:context=".Babouche1">

  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="?actionBarSize"
      android:orientation="vertical"
      tools:ignore="MissingConstraints">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="348dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-5dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="cursive"
        android:shadowColor="@color/black"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="5"
        android:text="Babouche pour femmes  "
        android:textColor="#03224c"
        android:textSize="30dp"
        android:textStyle="bold" />
  </RelativeLayout>



  <ImageView
      android:id="@+id/back2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginLeft="16dp"
      android:layout_marginTop="16dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/ic_arrow_back_black_24dp"
      android:contentDescription="@string/todo" />
  <ImageView
      android:id="@+id/image21"
      android:layout_width="456dp"
      android:layout_height="325dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentLeft="true"
      android:layout_alignParentTop="true"
      android:layout_marginTop="40dp"
      android:alpha="0"
      android:src="@drawable/art37"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.488"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/back2" />


  <ImageView
      android:id="@+id/big_image21"
      android:layout_width="590dp"
      android:layout_height="323dp"
      android:layout_marginTop="48dp"
      android:contentDescription="TODO"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.511"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/back2"
      app:srcCompat="@drawable/pantouffleblueroi"
      tools:ignore="ContentDescription" />

  <de.hdodenhof.circleimageview.CircleImageView
      android:id="@+id/pageofficiele25"
      android:layout_width="86dp"
      android:layout_height="60dp"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="36dp"
      android:layout_marginTop="452dp"
      android:outlineSpotShadowColor="@color/Darkgreen"
      android:src="@drawable/pantoufl"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/back2" />

  <de.hdodenhof.circleimageview.CircleImageView
      android:id="@+id/pageofficiele"
      android:layout_width="86dp"
      android:layout_height="60dp"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="124dp"
      android:layout_marginTop="452dp"
      android:outlineSpotShadowColor="@color/Darkgreen"
      android:src="@drawable/pantouffleblueroi"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/back2" />

  <de.hdodenhof.circleimageview.CircleImageView
      android:id="@+id/pageofficiele3"
      android:layout_width="86dp"
      android:layout_height="60dp"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="228dp"
      android:layout_marginTop="452dp"
      android:outlineSpotShadowColor="@color/Darkgreen"
      android:src="@drawable/pantoufllevlue"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/back2" />

  <de.hdodenhof.circleimageview.CircleImageView
      android:id="@+id/pageofficiele4"
      android:layout_width="86dp"
      android:layout_height="60dp"
      android:layout_alignParentEnd="true"
      android:layout_marginStart="312dp"
      android:layout_marginTop="452dp"
      android:outlineSpotShadowColor="@color/Darkgreen"
      android:src="@drawable/art2"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/back2" />

  <TextView
      android:id="@+id/productName"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="156dp"
      android:layout_marginTop="32dp"
      android:fontFamily="cursive"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/prodPrice"
      app:layout_constraintVertical_bias="0.777" />

  <TextView
      android:id="@+id/prodDesc"
      android:layout_width="155dp"
      android:layout_height="70dp"
      android:layout_marginTop="400dp"
      android:layout_marginEnd="232dp"
      android:fontFamily="cursive"
      android:text="@string/_45_0dt"
      android:shadowColor="@color/white"
      android:shadowDx="2"
      android:shadowDy="2"
      android:shadowRadius="5"
      android:textColor="@color/white"
      android:textSize="50sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/buttonpopup55"
      android:layout_width="199dp"
      android:layout_height="47dp"
      android:layout_marginStart="8dp"
      android:layout_marginEnd="16dp"
      android:background="@drawable/buy_now_bg"
      android:fontFamily="cursive"
      android:text="@string/contactez_le_vendeur"
      android:textAppearance="@style/TextAppearance.AppCompat.Medium"
      android:textColor="#FFFFFF"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView7"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toEndOf="@+id/imageView7"
      app:layout_constraintTop_toTopOf="@+id/imageView7"
      app:layout_constraintVertical_bias="0.0" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="90dp"
      android:layout_height="48dp"
      android:layout_marginStart="64dp"
      android:layout_marginBottom="16dp"
      android:contentDescription="@string/todo"
      app:circularflow_angles="20"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/prodPrice"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/shoppnow" />

  <TextView
      android:id="@+id/prodPrice"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="60dp"
      android:fontFamily="cursive"
      android:shadowColor="@color/black"
      android:shadowDx="2"
      android:shadowDy="2"
      android:shadowRadius="5"
      android:text=" Babbouche 'khomsa' pour femmes en     \n  cuir"
      android:textColor="#03224c"
      android:textSize="30sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/prodDesc"
      app:layout_constraintTop_toBottomOf="@+id/prodDesc" />

  <ImageView
      android:layout_width="90dp"
      android:layout_height="51dp"
      android:layout_marginStart="224dp"
      android:layout_marginBottom="216dp"
      android:src="@drawable/baliz"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <Button

      android:id="@+id/arjaa"
      android:layout_width="122dp"
      android:layout_height="51dp"
      android:layout_marginStart="260dp"
      android:layout_marginBottom="216dp"
      android:background="@color/Transp"
      android:fontFamily="cursive"
      android:text="Retour"
      android:textAppearance="@style/TextAppearance.AppCompat.Medium"
      android:textColor="@color/white"
      android:textSize="26dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>